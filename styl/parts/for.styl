#for
    width: 100%
    padding: 102px 0 100px
    background-image url(../img/for_bg.jpg)
    background-repeat no-repeat
    background-position 0 327px
    background-size: 95% 67.5%
.after_title
    font-size: 18px;
    color: rgb(29, 30, 46);
    line-height: 1.833;
    text-align: center;
    margin 24px auto 88px
    max-width 650px
.for_container
    text-align: center
    margin-bottom: 55px
.for_img
    margin 0 auto
    display block
    position relative
.for_img_first
    left -8px
.for_img_second
    top: -19px
    left: 6px;
    margin-bottom: -19px
.for_img_third
    left 4px
.for_img_fourth
    left -11px
.for_img_fifth
    left 16px
.for_title
    margin 25px 0 6px
    font-size: 16px;
    font-family: "OpenSansBold", sans-serif;
    color: rgb(173, 55, 41);
    line-height: 1.3;
.for_title + p
    line-height: 1.87;
    max-width 99%
    margin: 0 auto
.button_for
    margin-top: 14px